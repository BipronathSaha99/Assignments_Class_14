from math import sqrt
a=int(input("a="))
b=int(input("b="))
c=int(input("c="))

def equation(a,b,c):
    d=(pow(b,2)-4*a*c)

    if d==0:
        x=-b/(2*a)
        print(f"x={x:0.2f}")
    elif d>0:
        x_1=(-b+sqrt(d))/(2*a)
        x_2 = (-b - sqrt(d)) / (2 * a)
        print(f"x_1={x_1:0.2f} x_2={x_2:0.2f}",end="")
    else:
        print("The equation has imaginary solution")
equation(a,b,c)
